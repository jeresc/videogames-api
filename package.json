{
  "name": "back",
  "version": "1.0.0",
  "description": "PI",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
    "start:test": "NODE_ENV=test node src/index.js",
    "dev": "nodemon -L -q",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "imports": {
    "#app": "./src/app.js",
    "#routes": "./src/api/routes/index.js",
    "#models": "./src/api/models/index.js",
    "#services": "./src/api/services/index.js",
    "#schemas": "./src/api/schemas/index.js",
    "#controllers": "./src/api/controllers/index.js",
    "#middlewares": "./src/api/middlewares/index.js",
    "#config": "./src/config/config.js",
    "#helpers": "./src/api/helpers/index.js",
    "#adapters": "./src/api/adapters/index.js",
    "#auth": "./src/api/auth/index.js"
  },
  "author": "jeresc",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.6.2",
    "morgan": "^1.10.0",
    "node-dev": "^8.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "supertest-session": "^4.1.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1"
  }
}
